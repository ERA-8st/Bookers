<h1>Books</h1>
<table>
	<tr>
		<th>Title</th>
		<th>Body</th>
		<th></th>
	</tr>
	<% @books.each do |book| %>
	<tr>
		<td><%= book.title %></td>
		<td><%= book.body %></td>
		<td><%= link_to "Show", book_path(book.id) %></td>
		<td><%= link_to "Edit" %></td>
		<td><%= link_to "Destroy" %></td>
	</tr>
	<% end %>
</table>